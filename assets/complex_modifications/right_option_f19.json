{
	"title": "Map right_option to F19",
	"rules": [
		{
			"description": "Change right_option key to command+control+option+shift. (Post f19 key when pressed alone)",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "right_option",
						"modifiers": {
							"optional": ["any"]
						}
					},
					"to": [
						{
							"key_code": "left_shift",
							"modifiers": ["left_command", "left_control", "left_option"]
						}
					],
					"to_if_alone": [
						{
							"key_code": "f19"
						}
					]
				}
			]
		}
	]
}
